<% if order.delivery_date != nil %>
  <td class="align-center"><%= I18n.l order.delivery_date %></td>
<% else %>
  <td class="align-center">Unknown</td>
<% end %>
<% if order.delivery_slot != nil %>
  <td class="align-center"><%= order.delivery_slot.try(:name) %></td>
<% else %>
  <td class="align-center">Unknown</td>
<% end %>
